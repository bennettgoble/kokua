    </panel>
    <!--Performance 1-->
    <panel
        border="true"
        follows="all"
        height="408"
        label="Performance 1"
        left="1"
        mouse_opaque="true"
        name="kokua_performance_1"
        width="517">
      <check_box
           control_name="KokuaPerformanceRegionEnable"
           height="16"
           label="Region Entry Notification"
           left="20"
           name="KokuaPerformanceRegionEnable"
           top="-20"
           tool_tip="Enable notifications on entering a region"
           width="30"/>
      <check_box
           control_name="KokuaPerformanceRegionChannel"
           enabled_control="KokuaPerformanceRegionEnable"
           height="16"
           label="Channel"
           top="-40"
           left="30"
           name="KokuaPerformanceRegionChannel"
           tool_tip="Region entry notifications will include the server channel"
           width="30"/>
      <check_box
           control_name="KokuaPerformanceRegionAgent"
           enabled_control="KokuaPerformanceRegionEnable"
           height="16"
           label="Agents"
           top="-40"
           left="120"
           name="KokuaPerformanceRegionAgent"
           tool_tip="Region entry notifications will include agent information"
           width="30"/>
      <check_box
           control_name="KokuaPerformanceRegionScript"
           enabled_control="KokuaPerformanceRegionEnable"
           height="16"
           label="Scripts"
           top="-40"
           left="210"
           name="KokuaPerformanceRegionScript"
           tool_tip="Region entry notifications will include script information"
           width="80"/>
      <check_box
           control_name="KokuaPerformanceRegionTiming"
           enabled_control="KokuaPerformanceRegionEnable"
           height="16"
           label="Timing"
           top="-40"
           left="300"
           name="KokuaPerformanceRegionTiming"
           tool_tip="Region entry notifications will include timing information"
           width="30"/>
      <check_box
           control_name="KokuaPerformanceRegionBasics"
           enabled_control="KokuaPerformanceRegionEnable"
           height="16"
           label="Basics"
           top="-40"
           left="390"
           name="KokuaPerformanceRegionBasics"
           tool_tip="Region entry notifications will include basic performance indicators (FPS/TD)"
           width="30"/>
      <check_box
           control_name="KokuaPerformanceRegionRepeat"
           enabled_control="KokuaPerformanceRegionEnable"
           height="16"
           label="Repeat the region information when any performance notification is raised"
           left="20"
           name="KokuaPerformanceRegionRepeat"
           top="-70"
           tool_tip="The selected region entry information will be repeated when any performance notification is raised."
           width="450" />
      <check_box
           control_name="KokuaPerformanceRegionVersionChangeNotice"
           height="16"
           label="Notify if the simulator version changes while changing the region"
           left="20"
           name="KokuaPerformanceRegionVersionChangeNotice"
           top="-90"
           tool_tip="Displays a notice if the simulator version changes during a teleport or while crossing a region."
           width="450" />
      <check_box
           control_name="KokuaPerformanceNotifyRestore"
           height="16"
           label="Notify when a monitored statistic returns within the set threshold"
           left="20"
           name="KokuaPerformanceNotifyRestore"
           top="-110"
           tool_tip="Displays a message if a statistic returns wtih the set threshold - beware, this can double the number of messages generated!"
           width="450" />
      <check_box
           control_name="KokuaPerformanceAgentEnable"
           height="16"
           label="Give Agent Notifications"
           left="20"
           name="KokuaPerformanceAgentEnable"
           top="-140"
           tool_tip="Enable agent-related performance notifications"
           width="480" />
      <check_box
           control_name="KokuaPerformanceAgentRegionExceeds"
           enabled_control="KokuaPerformanceAgentEnable"
           height="16"
           label="When agents in region exceeds"
           top="-160"
           left="30"
           name="KokuaPerformanceAgentRegionExceeds"
           tool_tip="Give a message if number of agents in region goes over threshold"
           width="100"/>
      <check_box
           control_name="KokuaPerformanceAgentNearbyExceeds"
           enabled_control="KokuaPerformanceAgentEnable"
           height="16"
           label="When agents in nearby regions exceeds"
           left="30"
           top_delta="-20"
           name="KokuaPerformanceAgentNearbyExceeds"
           tool_tip="Give a message if number of agents in nearby regions goes over threshold"
           width="100"/>
      <check_box
           control_name="KokuaPerformanceAgentRegionChange"
           enabled_control="KokuaPerformanceAgentEnable"
           height="16"
